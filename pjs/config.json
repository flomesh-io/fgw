{
  "Configs": {
    "EnableDebug": true,
    "ShowUpstreamStatusInResponseHeader": true
  },
  "Consumers": [
    {
      "name": "test01",
      "password": "123456",
      "Headers-Authorization": {
        "authorization": "Basic dGVzdDoxMjM0NTY="
      }
    }
  ],
  "Listeners": [
    {
      "Protocol": "HTTP",
      "Port": 8080,
      "AccessControlLists": {
        "blacklist": [],
        "whitelist": []
      }
    },
    {
      "Protocol": "HTTP",
      "Port": 8081
    },
    {
      "Protocol": "HTTP",
      "Port": 8082
    }
  ],
  "RouteRules": {
    "8080": {
      "*": {
        "RouteType": "HTTP",
        "Matches": [
          {
            "route": "route-id-001",
            "Path": {
              "Type": "Prefix",
              "Path": "/"
            },
            "BackendService": {
              "backendService1": 100
            }
          }
        ]
      }
    },
    "8081": {
      "*": {
        "Matches": [
          {
            "ServerRoot": "www1",
            "Index": [
              "index.html",
              "index.htm"
            ],
            "TryFiles": [
              "$uri",
              "$uri/default/",
              "=404"
            ]
          }
        ]
      }
    },
    "8082": {
      "*": {
        "Matches": [
          {
            "ServerRoot": "www2",
            "Index": [
              "default.html",
              "index.htm"
            ]
          }
        ]
      }
    }
  },
  "Services": {
    "backendService1": {
      "StickyCookieName": "_srv_id",
      "StickyCookieExpires": 3600,
      "HealthCheck": {
        "Interval": 10,
        "MaxFails": 3,
        "Uri": "/",
        "Matches": [
          {
            "Type": "status",
            "Value": [
              200,
              201
            ]
          }
        ]
      },
      "Algorithm": "LeastConnectionLoadBalancer",
      "Endpoints": {
        "127.0.0.1:8081": {
          "Weight": 50
        },
        "127.0.0.1:8082": {
          "Weight": 50
        },
        "127.0.0.1:8083": {
          "Weight": 50
        }
      }
    }
  },
  "Chains": {
    "HTTPRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/consumer.js",
      "http/codec.js",
      "http/auth.js",
      "http/route.js",
      "http/service.js",
      "http/metrics.js",
      "http/tracing.js",
      "http/logging.js",
      "http/access-control-domain.js",
      "http/access-control-route.js",
      "http/circuit-breaker.js",
      "http/throttle-domain.js",
      "http/throttle-route.js",
      "filter/request-redirect.js",
      "filter/header-modifier.js",
      "filter/url-rewrite.js",
      "http/forward.js",
      "http/default.js"
    ],
    "HTTPSRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/tls-termination.js",
      "common/consumer.js",
      "http/codec.js",
      "http/auth.js",
      "http/route.js",
      "http/service.js",
      "http/metrics.js",
      "http/tracing.js",
      "http/logging.js",
      "http/access-control-domain.js",
      "http/access-control-route.js",
      "http/circuit-breaker.js",
      "http/throttle-domain.js",
      "http/throttle-route.js",
      "filter/request-redirect.js",
      "filter/header-modifier.js",
      "filter/url-rewrite.js",
      "http/forward.js",
      "http/default.js"
    ],
    "TLSPassthrough": [
      "common/access-control.js",
      "common/ratelimit.js",
      "tls/passthrough.js",
      "common/consumer.js"
    ],
    "TLSTerminate": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/tls-termination.js",
      "common/consumer.js",
      "tls/forward.js"
    ],
    "TCPRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "tcp/forward.js"
    ]
  },
  "Version": "0"
}
