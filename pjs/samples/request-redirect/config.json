{
  "Configs": {
    "EnableDebug": true
  },
  "Listeners": [
    {
      "Protocol": "HTTP",
      "Port": 6000
    }
  ],
  "RouteRules": {
    "6000": {
      "*": {
        "RouteType": "HTTP",
        "Matches": [
          {
            "Filters": [
              {
                "Type": "RequestRedirect",
                "RequestRedirect": {
                  "Scheme": "https",
                  "Hostname": "www.flomesh.cn",
                  "Port": 443,
                  "Path": "/",
                  "StatusCode": 301
                }
              }
            ],
            "BackendService": {
              "emptyService": {
                "Weight": 100
              }
            }
          }
        ]
      }
    }
  },
  "Services": {
    "emptyService": {}
  },
  "Chains": {
    "HTTPRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/consumer.js",
      "http/codec.js",
      "http/access-log.js",
      "http/auth.js",
      "http/route.js",
      "http/service.js",
      "http/metrics.js",
      "http/tracing.js",
      "http/logging.js",
      "http/circuit-breaker.js",
      "http/throttle-domain.js",
      "http/throttle-route.js",
      "filter/request-redirect.js",
      "filter/header-modifier.js",
      "filter/url-rewrite.js",
      "http/error-page.js",
      "http/proxy-redirect.js",
      "http/forward.js",
      "http/default.js"
    ],
    "HTTPSRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/tls-termination.js",
      "common/consumer.js",
      "http/codec.js",
      "http/access-log.js",
      "http/auth.js",
      "http/route.js",
      "http/service.js",
      "http/metrics.js",
      "http/tracing.js",
      "http/logging.js",
      "http/circuit-breaker.js",
      "http/throttle-domain.js",
      "http/throttle-route.js",
      "filter/request-redirect.js",
      "filter/header-modifier.js",
      "filter/url-rewrite.js",
      "http/error-page.js",
      "http/proxy-redirect.js",
      "http/forward.js",
      "http/default.js"
    ],
    "TLSPassthrough": [
      "common/access-control.js",
      "common/ratelimit.js",
      "tls/passthrough.js",
      "common/consumer.js"
    ],
    "TLSTerminate": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/tls-termination.js",
      "common/consumer.js",
      "tls/forward.js"
    ],
    "TCPRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "tcp/forward.js"
    ]
  },
  "Version": "0"
}