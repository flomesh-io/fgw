{
  "Configs": {
    "DefaultPassthroughUpstreamPort": 443,
    "EnableDebug": true
  },
  "Listeners": [
    {
      "Protocol": "HTTP",
      "Port": 80,
      "AccessControlLists": {
        "blacklist": [
          "127.0.0.11",
          "192.168.122.1/32"
        ],
        "whitelist": [
          "192.168.122.18",
          "127.0.0.11/32"
        ]
      },
      "bpsLimit": 10000000
    },
    {
      "Protocol": "HTTP",
      "Port": 81,
      "AccessControlLists": {
        "blacklist": [
          "192.168.122.18",
          "127.0.0.11/32"
        ]
      }
    }
  ],
  "RouteRules": {
    "80": {
      "test.com": {
        "RouteType": "HTTP",
        "Matches": [
          {
            "Path": {
              "type": "Prefix",
              "path": "/path"
            },
            "Headers": {
              "type": "Exact",
              "headers": [
                {
                  "a": "1",
                  "b": "2"
                },
                {
                  "c": "3"
                }
              ]
            },
            "Methods": [
              "GET",
              "POST"
            ],
            "QueryParams": {
              "type": "Exact",
              "params": [
                {
                  "abc": "1"
                }
              ]
            },
            "BackendService": {
              "www8088": 100
            }
          }
        ]
      },
      "*.test.com": {
        "RouteType": "HTTP",
        "Matches": [
          {
            "Path": {
              "type": "Prefix",
              "path": "/repo"
            },
            "Headers": {
              "type": "Exact",
              "headers": [
                {
                  "b": "2"
                }
              ]
            },
            "Methods": [
              "GET",
              "POST"
            ],
            "QueryParams": {
              "type": "Exact",
              "params": [
                {
                  "abc": "1"
                }
              ]
            },
            "BackendService": {
              "www8088": 100
            }
          }
        ]
      }
    }
  },
  "Services": {
    "www8088": {
      "Endpoints": {
        "127.0.0.1:8088": {
          "Weight": 100,
          "Tags": {
            "Cluster": "cluster1"
          }
        }
      }
    }
  },
  "Chains": {
    "HTTPRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/consumer.js",
      "http/codec.js",
      "http/auth.js",
      "http/route.js",
      "http/service.js",
      "http/metrics.js",
      "http/tracing.js",
      "http/logging.js",
      "http/circuit-breaker.js",
      "http/throttle-domain.js",
      "http/throttle-route.js",
      "filter/request-redirect.js",
      "filter/header-modifier.js",
      "filter/url-rewrite.js",
      "http/forward.js",
      "http/default.js"
    ],
    "HTTPSRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/tls-termination.js",
      "common/consumer.js",
      "http/codec.js",
      "http/auth.js",
      "http/route.js",
      "http/service.js",
      "http/metrics.js",
      "http/tracing.js",
      "http/logging.js",
      "http/circuit-breaker.js",
      "http/throttle-domain.js",
      "http/throttle-route.js",
      "filter/request-redirect.js",
      "filter/header-modifier.js",
      "filter/url-rewrite.js",
      "http/forward.js",
      "http/default.js"
    ],
    "HTTPSPassthrough": [
      "common/access-control.js",
      "common/ratelimit.js",
      "https/passthrough.js",
      "common/consumer.js"
    ],
    "TLSTerminate": [
      "common/access-control.js",
      "common/ratelimit.js",
      "common/tls-termination.js",
      "common/consumer.js",
      "tls/forward.js"
    ],
    "TCPRoute": [
      "common/access-control.js",
      "common/ratelimit.js",
      "tcp/forward.js"
    ]
  },
  "Features": {
    "Logging": {},
    "Tracing": {},
    "Metrics": {}
  }
}